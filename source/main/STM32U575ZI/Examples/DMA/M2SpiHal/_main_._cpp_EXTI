void GPDMA1_Channel7_IRQHandler(void)
{
  GPDMA1_Channel7->CCR &= ~DMA_CCR_EN;
  GPDMA1_Channel7->CFCR = DMA_CFCR_TCF;
  GPIOE->ODR |= (1 << 1);
}

