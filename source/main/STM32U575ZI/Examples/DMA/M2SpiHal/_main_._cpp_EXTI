void EXTI5_IRQHandler(void)
{

  if (__HAL_GPIO_EXTI_GET_IT(LL_EXTI_LINE_5) != 0x00u)
  {
    __HAL_GPIO_EXTI_CLEAR_IT(LL_EXTI_LINE_5);
    if (GPIOE->IDR & (1 << 0))
    {
      GPIOE->ODR &= ~(1 << 0);
    }
    else
    {
      GPIOE->ODR |= (1 << 0);
    }
  }
}


