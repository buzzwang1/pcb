                    <description>No index event occurred.</description>
                    <value>0x0</value>
                  </enumeratedValue>
                  <enumeratedValue>
                    <name>B_0x1</name>
                    <description>An index event has occurred</description>
                    <value>0x1</value>
                  </enumeratedValue>
